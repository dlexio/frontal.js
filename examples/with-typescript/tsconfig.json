{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["./assets/*"]
		},
		"target": "ES5",
		"module": "CommonJS",
		"lib": ["ES5", "ScriptHost", "dom"],
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"strict": true,
		"outDir": ".frontal",
		"forceConsistentCasingInFileNames": true
	},
	"include": ["./assets"],
	"exclude": ["node_modules"]
}